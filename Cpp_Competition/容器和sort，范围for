#include<bits/stdc++.h>
using namespace std;
using ll=long long;
using pii=pair<int,int>;



int main()
{
    ios::sync_with_stdio(false);
    cin.tie(0);
    cout.tie(0);
    int T;
    cin>>T;
    while(T--){
    	int n;
    	cin>>n;
    	int cnt[26]={0};
    	string s;
    	cin >> s;
    	for(char c:s){
    		cnt[c-'a']++;
    	}
    	vector<int> a;
    	for(int i=0;i<26;i++){
    		if(cnt[i]){
    			a.push_back(cnt[i]);
    		}
    	}
    	sort(a.begin(),a.end());
    	bool ok = true;
    	for(int i = 1;i<a.size();i++){
    		if(a[i] != a[i-1]+1){
    			ok=false;
    			break;
    		}
    	}
    	if(ok){
    		cout<<"YES\n";
    	}
    	else
    	cout<<"NO\n";
    	}

    

    
    
    return 0;
}