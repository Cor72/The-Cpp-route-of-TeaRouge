#include<bits/stdc++.h>
using namespace std;
using ll=long long;
using pii=pair<int,int>;

int game(int a,int b)
{
    if(a==b)
    return 24;
    else if(b>a)
    return b-a;
    else if(a>b)
    return 24-a+b;
    
}
int main()
{
    ios::sync_with_stdio(false);
    cin.tie(0);
    cout.tie(0);
    
    int A,B;
    cin>>A>>B;
    cout<<"O JOGO DUROU "<<game(A,B)<<" HORA(S)";
    return 0;
}