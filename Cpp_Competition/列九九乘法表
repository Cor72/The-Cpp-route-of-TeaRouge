#include<bits/stdc++.h>
using namespace std;
using ll=long long;
using pii=pair<int,int>;



int main()
{
    ios::sync_with_stdio(false);
    cin.tie(0);
    cout.tie(0);
    int n,m;
    cin>>n>>m;
    for(int i=1;i<=m;i++){
    	for(int j=1;j<=i&&j<=n;j++){
    		cout<<j<<"x"<<i<<"="<<j*i;
    		if (j + 1 <= i && j + 1 <= n) cout << ' ';
    	}
    	cout<<'\n';
    }

    return 0;
}